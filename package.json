{
  "name": "store-checkout",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "start:build": "npm run build && serve -s build",
    "test": "react-scripts test --verbose",
    "test:update": "npm run test -- updateSnapshot",
    "test:ci": "npm test -- --no-watch --coverage",
    "lint": "eslint \"src/**/*.js\"",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --list-different \"src/**/*.js\"",
    "precommit": "lint-staged",
    "lh": "lighthousebot --pwa=40 --perf=90 --a11y=75 --bp=86 --seo=85 https://store.buildit.digital",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:import/errors",
      "plugin:import/warnings",
      "prettier"
    ],
    "plugins": [
      "import"
    ],
    "rules": {
      "no-console": [
        "error"
      ]
    }
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 5,
        "lines": 5,
        "statements": 5
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "printWidth": 120
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@sentry/browser": "4.6.5",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-redux": "6.0.1",
    "react-router-dom": "5.0.0",
    "react-scripts": "2.1.8",
    "redux": "4.0.1",
    "styled-components": "4.2.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "husky": "1.3.1",
    "jest-dom": "3.1.3",
    "jest-styled-components": "6.3.1",
    "lighthousebot": "git+https://github.com/GoogleChromeLabs/lighthousebot.git",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "prop-types": "15.7.2",
    "react-test-renderer": "16.8.6",
    "react-testing-library": "6.0.3",
    "redux-devtools-extension": "2.13.8",
    "serve": "10.1.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
