{
  "name": "store-checkout",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "start:build": "npm run build && serve -s build",
    "test": "react-scripts test --verbose",
    "test:update": "npm run test -- --no-watch --updateSnapshot",
    "test:ci": "npm test -- --no-watch --coverage",
    "e2e:dev": "node -e 'require(\"./src/e2eConfig\").e2eRunDev(\"http://localhost:3000\")'",
    "e2e": "npm run build && node -e 'require(\"./src/e2eConfig\").e2eRun(\"5000\")'",
    "lint": "eslint \"src/**/*.js\"",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --list-different \"src/**/*.js\"",
    "precommit": "lint-staged",
    "lh": "lighthousebot --pwa=50 --perf=95 --a11y=95 --bp=95 --seo=85 https://store.buildit.digital",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:import/errors",
      "plugin:import/warnings",
      "prettier"
    ],
    "plugins": [
      "import"
    ],
    "rules": {
      "no-console": [
        "error"
      ]
    }
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!<rootDir>/node_modules/",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!src/e2eConfig.js",
      "!src/components/Root/*",
      "!src/constants/*"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "printWidth": 120
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@sentry/browser": "4.6.5",
    "lodash.noop": "3.0.1",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-redux": "6.0.1",
    "react-router-dom": "5.0.0",
    "react-scripts": "2.1.8",
    "redux": "4.0.1",
    "styled-components": "4.2.0"
  },
  "devDependencies": {
    "cypress": "3.2.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "husky": "1.3.1",
    "jest-dom": "3.1.3",
    "jest-styled-components": "6.3.1",
    "lighthousebot": "git+https://github.com/GoogleChromeLabs/lighthousebot.git",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "prop-types": "15.7.2",
    "react-test-renderer": "16.8.6",
    "react-testing-library": "6.0.3",
    "redux-devtools-extension": "2.13.8",
    "serve": "11.0.0",
    "serve-handler": "5.0.8"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
