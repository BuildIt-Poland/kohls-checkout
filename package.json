{
  "name": "store-checkout",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --verbose",
    "test:ci": "npm test -- --no-watch --coverage",
    "e2e:dev": "npm-run-all --parallel --race start 'cy:open -- --config baseUrl=http://localhost:3000'",
    "e2e": "npm run build && npm run cy:start-server-and-test",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:start-server-and-test": "start-server-and-test serve :5000 cy:run",
    "serve": "serve --no-clipboard --single --listen 5000 build",
    "lint": "eslint \"src/**/*.js\"",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --list-different \"src/**/*.js\"",
    "precommit": "lint-staged",
    "eject": "react-scripts eject",
    "test1": "node e2eConfig.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:import/errors",
      "plugin:import/warnings",
      "prettier"
    ],
    "plugins": [
      "import"
    ],
    "rules": {
      "no-console": [
        "error"
      ]
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 5,
        "lines": 5,
        "statements": 5
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "printWidth": 120
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-redux": "6.0.1",
    "react-router-dom": "5.0.0",
    "react-scripts": "2.1.8",
    "redux": "4.0.1",
    "styled-components": "4.2.0"
  },
  "devDependencies": {
    "cypress": "3.2.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "enzyme-to-json": "3.3.5",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "husky": "1.3.1",
    "jest-styled-components": "6.3.1",
    "lint-staged": "8.1.5",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.4",
    "prop-types": "15.7.2",
    "redux-devtools-extension": "2.13.8",
    "serve": "10.1.2",
    "start-server-and-test": "1.7.12"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
